{
    "name": "sobanvuex/lumen-bin-cli",
    "description": "Command line interface for lumen-bin",
    "keywords": ["lumen-bin", "lumen", "bin", "pastebin", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Soban",
            "email": "me@soban.co"
        }
    ],
    "require": {
        "bacon/bacon-qr-code": "^2.0",
        "hassankhan/config": "^2.0",
        "symfony/console": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SobanVuex\\LumenBin\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SobanVuex\\LumenBin\\Console\\Tests\\": "tests/"
        }
    },
    "bin": [
        "lu-bin"
    ],
    "config": {
        "sort-packages": true
    }
}
